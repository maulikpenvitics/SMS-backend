 @model WebUI.Models.VendorContractViewModel
 @{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 <script>
        (function () {
             ("form").validate();
        });
    </script>
    <div class="container">
        <div class="row">
         <br />
            <br />
            <div class="col-12">
                <a href="@Url.Action("List","VendorContract")" class="btn btn-success">VendorContract List</a>
            </div>
              <br />
            <div class="col-12">
                @using (Html.BeginForm("Submit", "VendorContract", FormMethod.Post))
                {
                    <div class="col-sm-12">
                    <div class="form-group" hidden>
                        <label>Id</label>
                         @Html.TextBoxFor(model => model.Id, new { @class = "form-control" })
                    </div>
                    <div class="col-sm-4"><div class="form-group">
<label>vendor</label>
@Html.DropDownListFor(model => model.VendorId, Model.Dropdownvendor," Select vendor", new { @class="form-control" })
@Html.ValidationMessageFor(model => model.VendorId, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Contracturl</label>
@Html.TextBoxFor(model => model.Contracturl, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Contracturl, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Fromdate</label>
@Html.TextBoxFor(model => model.Fromdate, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Fromdate, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Todate</label>
@Html.TextBoxFor(model => model.Todate, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Todate, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Amount</label>
@Html.TextBoxFor(model => model.Amount, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Amount, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Paymentcycle</label>
@Html.TextBoxFor(model => model.Paymentcycle, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Paymentcycle, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Modifiedby</label>
@Html.TextBoxFor(model => model.Modifiedby, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Modifiedby, null, new { @class="error" })
</div>
</div>
<div class="col-sm-4"><div class="form-group">
<label>Modifieddate</label>
@Html.TextBoxFor(model => model.Modifieddate, new { @class="form-control" })
@Html.ValidationMessageFor(model => model.Modifieddate, null, new { @class="error" })
</div>
</div>

                    @if (ViewBag.error != "")
                    {
                        <div>
                            @ViewBag.error
                        </div>
                    }
                    </div>
                    <div class="col-sm-12">
                    <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                }
            </div>
        </div>
    </div>
 
